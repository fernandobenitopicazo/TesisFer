@article{Rodriguez2005,
abstract = {This article presents information on a newly developed application of knowledge discovery technology, named "Knowledge Discovering from Databases (KDD)," for areas like bioinformatics. Discovering homologies and evolutionary relationships between genome sequences is considered as the natural way of assigning function to unknown sequences. Several tools are routinely used to this end, from heuristic approaches to more exhaustive algorithms based on dynamic programming strategies. The key element in these strategies is to detect the presence of strong signatures shared by sequences, from which it is possible to infer that those sequences also share a common functionality but these have some limitations. For addressing these limitations, the authors have developed the said KDD technology. The goal of the KDD process, reportedly, is the extraction and abstraction of any type of pattern, perturbation, relationship or association from the analyzed data. One of most useful KDD techniques is the use of association rules to show the relationship between a set of antecedents and its associated consequents. This article addresses the problem of functional assignment to proteins that share short similar fragments with proteins of known function, by the use of association rules and describes the usefulness of the KDD.},
annote = {Habla de bastantes m{\'{e}}todos de miner{\'{i}}a de asociaciones para datasets biol{\'{o}}gicos},
author = {Rodriguez, Andres and Carazo, Jose Maria and Trelles, Oswaldo},
doi = {10.1002/asi.20138},
file = {::},
journal = {Journal of the American Society for Information Science and Technology},
title = {{Mining association rules from biological databases}},
pages = {493--504},
year = {2005}
}


